const int sensorPin = A0;
 
void setup() {
   Serial.begin(9600);
}
 
void loop() 
{
   int humedad = analogRead(sensorPin);
   Serial.print(humedad);
  
   if(humedad < 500)
   {
      Serial.println("Planta hÃºmeda");  
      //hacer las acciones necesarias
   }else{
   Serial.println("Planta seca");
   }
   delay(1000);
}
